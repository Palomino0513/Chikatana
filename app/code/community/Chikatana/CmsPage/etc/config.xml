<?xml version="1.0" encoding="UTF-8"?>
<config>
    <modules>
        <Chikatana_CmsPage>
            <version>0.0.1</version>
        </Chikatana_CmsPage>
    </modules>
    <global>
        <helpers>
            <cmspage>
                <class>Chikatana_CmsPage_Helper</class>
            </cmspage>
        </helpers>
        <blocks>
            <cmspage>
                <class>Chikatana_CmsPage_Block</class>
            </cmspage>
        </blocks>
    </global>
    <frontend>
        <routers>
            <cmspage>
                <use>standard</use>
                <args>
                    <module>Chikatana_CmsPage</module>
                    <frontName>cmspage</frontName>
                </args>
            </cmspage>
        </routers>
        <layout>
            <updates>
                <cmspage>
                    <file>cmspage.xml</file>
                </cmspage>
            </updates>
        </layout>
    </frontend>
</config>
